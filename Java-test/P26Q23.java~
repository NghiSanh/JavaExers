public class P26Q23
{
    public static void main(){
        int a = 1;
        int b = 0;
        for(int i = 0; i < 11; i++){
            b += i;
            
    }
    System.out.println(b);
    
    while(a++ <= 10){
        int sum = 0;
        sum += a;
    }
    System.out.println(sum);
}
}