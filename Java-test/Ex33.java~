public class Ex33
{
    public static void main(){
        
        for(int y = 0; y < 5; y++){
            for(int x = 0; x < 9; x++){
                if(x == 4 || x + y == 4 ){
                    System.out.print("*");
                }
                /*else if( x + y == 4 || x + y == 6){
                    System.out.print("*";
                }*/
                else{
                    System.out.print(" ");
                }
                
            }
            
            System.out.println();
        }
    }
}
